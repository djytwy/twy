#!/usr/bin/env python
#coding:utf-8

class Duck:
    def jiao(self):
        print '呱呱呱'
    def waibiao(self):
        print '灰色的鸭毛'
    def youyong(self):
        print 'swiming'

class Person:
    def jiao(self):
        print '我不是鸭子'
    def waibiao(self):
        print '鸭毛羽绒服'
    def youyong(self):
        print '游不来'

def duck_person(Person):
    Person.jiao()
    Person.waibiao()
    Person.youyong()

def run():
    yazi = Duck()
    ren = Person()
    duck_person(yazi)
    duck_person(ren)

run()

